    "def countRotations(arr):\n",
    "    min = arr[0]\n",
    "    min_index = 0\n",
    "    for i in range(1,len(arr)):\n",
    "        if (min > arr[i]):\n",
    "            min = arr[i]\n",
    "            min_index = i\n",
    "    return min_index\n",
    "print(countRotations((15,18,2,3,6,12)))"
   ]
  }
